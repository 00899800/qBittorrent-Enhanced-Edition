name: python
-----END QCMOD-----
*/
class qc_python : public ConfObj
{
public:
	qc_python(Conf *c) : ConfObj(c) {}
	QString name() const { return "python >= 2.3"; }
	QString shortname() const { return "python"; }
	bool exec(){
          int r = conf->doCommand("python testpython.py");
	  if(r == 0)
	    return true;
	  else
	    return false;
	}
};
